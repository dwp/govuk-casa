{
  "report-dir": ".coverage",
  "produce-source-map": true,
  "cache": true,
  "all": true,
  "check-coverage": true,
  "per-file": true,
  "lines": 60,
  "statements": 0,
  "functions": 0,
  "branches": 0,
  "watermarks": {
    "lines": [60, 80],
    "statements": [60, 80],
    "functions": [60, 80],
    "branches": [60, 80]
  },
  "include": ["src/**/*.js"],
  "exclude": [
    "node_modules/**",
    "src/lib/**/index.js",
    "src/casa.js",
    "tests/**"
  ],
  "reporter": ["text", "lcov"]
}
